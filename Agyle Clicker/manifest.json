{
    "manifest_version": 2,
    "name": "Agyle Status Selector",
    "version": "1.0",
    "description": "A Chrome extension that automatically clicks a users Agyle status.",
    "permissions": [
        "activeTab", 
        "tabs"
    ],
    "content_scripts": [
        {
          "matches": ["https://mailchimp.zendesk.com/agent/dashboard"],
          "js": ["content_script.js"]
        }
    ]
}